page {
  box-sizing: border-box;
  background-color: #f6f6f6;
}
.container {
  padding: 40rpx;
}

.items {
  display: flex;
  text-align: left;
  width: 100%;
  height: 50px;
  box-sizing: border-box;
  border-bottom: 0.5px solid #dddddd;
}

.fromString {
  max-width: 250rpx;
  line-height: 50px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-right: 10px;
}

.toString {
  flex:1;
  color:#999999;
  font-size: 15px;
  line-height: 50px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap
}
.itemClearButton {
    color:#999999;
    line-height: 50px;
}

.allClearButton {
  margin: 10px;
  padding: 5px 20px;
  background-color: white;
  border-radius: 50px;
}